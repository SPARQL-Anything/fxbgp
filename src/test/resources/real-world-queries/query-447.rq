# https://api.github.com/repositories/303967701/contents/sparql-anything-it/src/test/resources/constructResource.sparql?ref=23626c6522e626253fd88da4a8674fd46004d205
PREFIX fx: <http://sparql.xyz/facade-x/ns/>

CONSTRUCT {
    ?s ?p ?o
} WHERE {
    SERVICE <x-sparql-anything:> {
        fx:properties fx:location "%%%LOCATION%%%" .
        ?s ?p ?o .
    }
}