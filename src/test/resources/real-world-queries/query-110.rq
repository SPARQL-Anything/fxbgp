# https://api.github.com/repositories/303967701/contents/sparql-anything-it/src/test/resources/issues/issue256.sparql?ref=23626c6522e626253fd88da4a8674fd46004d205
PREFIX fx: <http://sparql.xyz/facade-x/ns/>
CONSTRUCT 
  { 
    ?s ?p ?o .
  }
WHERE
  { SERVICE <x-sparql-anything:location=%%location%%>
      { fx:properties
                  fx:json.path.1  "$[?(@.name==\"Friends\")].stars" ;
                  fx:json.path.2  "$[?(@.name==\"Cougar Town\")].stars" . ?s  ?p  ?o }
  }