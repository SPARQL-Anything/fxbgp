# https://api.github.com/repositories/303967701/contents/sparql-anything-it/src/test/resources/issues/issue180-csv.sparql?ref=23626c6522e626253fd88da4a8674fd46004d205
PREFIX fx: <http://sparql.xyz/facade-x/ns/>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
SELECT DISTINCT ?fred ?sally
WHERE {
   SERVICE <x-sparql-anything:location=%%%LOCATION%%%>
   {
      fx:properties fx:csv.headers true .
      fx:properties fx:csv.ignore-columns-with-no-header true .

      ?root a fx:root ;
                  rdf:_1 [rdf:_1 ?fred] ;
                  rdf:_2 [rdf:_1 ?sally] .
   }
}