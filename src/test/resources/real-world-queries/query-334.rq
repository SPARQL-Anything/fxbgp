# https://api.github.com/repositories/303967701/contents/sparql-anything-it/src/test/resources/issues/issue295.sparql?ref=23626c6522e626253fd88da4a8674fd46004d205
PREFIX  xyz:  <http://sparql.xyz/facade-x/data/>
PREFIX  fx:   <http://sparql.xyz/facade-x/ns/>
PREFIX  rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>

SELECT  ?name ?surname ?movie
WHERE
  { SERVICE <x-sparql-anything:location=%%%LOCATION%%%,slice=true,ondisk=%%%TDB_PATH%%%,ondisk.reuse=false>
      { ?p  xyz:name     ?name ;
            xyz:surname  ?surname ;
            xyz:movie    ?movie
      }
  }