# https://api.github.com/repositories/598502070/contents/append/construct.sparql?ref=53dcc5377587eb7f804ed54c9be1acb3dc26a434
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX xyz: <http://sparql.xyz/facade-x/data/>
PREFIX fx:  <http://sparql.xyz/facade-x/ns/>

construct {
 ?subject xyz:key ?key ; 
 	xyz:value ?value .
}
where {
	service <x-sparql-anything:> {
		fx:properties fx:location "./data.json" .		
		[] xyz:key ?_key_int ; xyz:value ?value .
	}
	BIND (?_key_int as ?key) .
	BIND ( fx:entity(xyz:,"append/", ?key , "/" , STR(NOW()) ) as ?subject ) .
}