# https://api.github.com/repositories/303967701/contents/sparql-anything-it/src/test/resources/issues/issue260.sparql?ref=23626c6522e626253fd88da4a8674fd46004d205
PREFIX  xyz:  <http://sparql.xyz/facade-x/data/>
PREFIX  fx:   <http://sparql.xyz/facade-x/ns/>

SELECT  ?name ?surname
WHERE
  { SERVICE <x-sparql-anything:location=%%location%%,csv.headers=true,strategy=1>
      { fx:properties
                  fx:csv.null-string  "" .
        ?c        xyz:name            ?name ;
                  xyz:surname         ?surname
        FILTER NOT EXISTS { ?c  xyz:email  ?email }
      }
  }