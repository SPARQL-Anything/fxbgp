# https://api.github.com/repositories/303967701/contents/sparql-anything-it/src/test/resources/commands/logs.sparql?ref=23626c6522e626253fd88da4a8674fd46004d205
PREFIX owl:  <http://www.w3.org/2002/07/owl#>
PREFIX rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX fx:   <http://sparql.xyz/facade-x/ns/>
PREFIX xyz:  <http://sparql.xyz/facade-x/data/>


CONSTRUCT { ?x ?y ?z }
WHERE {
	SERVICE <x-sparql-anything:> {
		fx:properties fx:command "/usr/bin/git --no-pager log --date=local --all --pretty=%h,%an,%ad,'%s'" ;
		    fx:charset "ASCII" ;
		    fx:csv.delimiter "," ;
		    fx:csv.null-string "" ;
		    fx:csv.quote-char "'" ;
			fx:media-type "text/csv" .
		?x ?y ?z
	}
}





